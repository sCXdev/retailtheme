
{{#if settings.show_cta}}
<div class="footer-cta-wrap {{#is settings.cta_background_type 'cta_image'}}cta-image{{/is}}">
  <div class="container ">
    <div class="footer-cta">
      <aside>
        <h3>{{#if settings.use_dc}}{{dc settings.cta_title}}{{else}}{{settings.cta_title}}{{/if}}</h3>
        {{#if settings.cta_description}}
        <h4>{{#if settings.use_dc}}{{dc settings.cta_description}}{{else}}{{settings.cta_description}}{{/if}}</h4>
        {{/if}}
      </aside>
      <a href='{{settings.cta_button_link}}'>
        <h4>{{#if settings.use_dc}}{{dc settings.cta_button_text}}{{else}}{{settings.cta_button_text}}{{/if}}
        </h4>
      </a>
    </div>
  </div>
</div>
{{/if}}

<footer class="footer">
  <div class="footer-inner footer-custom">
        {{#if settings.footer_item_1_name}}
          <a href="{{settings.footer_item_1_link}}">
            {{#if settings.use_dc}}{{dc settings.footer_item_1_name}}{{else}}{{settings.footer_item_1_name}}{{/if}}
            </a>
        {{/if}}
        {{#if settings.footer_item_2_name}}
          <a href="{{settings.footer_item_2_link}}">
            {{#if settings.use_dc}}{{dc settings.footer_item_2_name}}{{else}}{{settings.footer_item_2_name}}{{/if}}</a>
        {{/if}}
        {{#if settings.footer_item_3_name}}
          <a href="{{settings.footer_item_3_link}}">
            {{#if settings.use_dc}}{{dc settings.footer_item_3_name}}{{else}}{{settings.footer_item_3_name}}{{/if}}
          </a>
        {{/if}}
        {{#if settings.footer_item_4_name}}
          <a href="{{settings.footer_item_4_link}}">
            {{#if settings.use_dc}}{{dc settings.footer_item_4_name}}{{else}}{{settings.footer_item_4_name}}{{/if}}
          </a>
        {{/if}}
        {{#if settings.footer_item_5_name}}
          <a href="{{settings.footer_item_5_link}}">
            {{#if settings.use_dc}}{{dc settings.footer_item_5_name}}{{else}}{{settings.footer_item_5_name}}{{/if}}
          </a>
        {{/if}}

  </div>
     
        <div class="footer-inner copr">
           {{#if settings.footer_copyright}}
          <span>{{#if settings.use_dc}}{{dc settings.footer_copyright}}{{else}}{{settings.footer_copyright}}{{/if}}
